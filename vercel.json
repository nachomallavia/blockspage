{
    "$schema": "https://openapi.vercel.sh/vercel.json",

    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "www.:subdomain.maindomain.com"
                }
            ],
            "destination": "https://:subdomain.maindomain.com/:1",
            "permanent": true
        },
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": ":subdomain.maindomain.com"
                },
                {
                    "type": "protocol",
                    "value": "http"
                }
            ],
            "destination": "https://:subdomain.maindomain.com/:1",
            "permanent": true
        }
    ]
}

{
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "www.:subdomain.blockspage.com"
                }
            ],
            "destination": "https://:subdomain.blockspage.com/:1",
            "permanent": true
        },
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": ":subdomain.blockspage.com"
                },
                {
                    "type": "protocol",
                    "value": "http"
                }
            ],
            "destination": "https://:subdomain.blockspage.com/:1",
            "permanent": true
        }
    ]
}
